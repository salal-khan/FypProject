<!DOCTYPE html>
 <html lang="en">

<head>
    <meta charset="UTF-8">
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, shrink-to-fit=no" name="viewport">
    <title>FYP :: Home</title>
    <!-- General CSS Files -->
    <link rel="stylesheet" href="~/FrontEnd/Theme/css/app.min.css">
    <link rel="stylesheet"  href="~/FrontEnd/Theme/bundles/lightgallery/dist/css/lightgallery.css">
    <link rel="stylesheet" href="~/FrontEnd/Theme/bundles/bootstrap-daterangepicker/daterangepicker.css">
    <link rel="stylesheet" href="~/FrontEnd/Theme/bundles/bootstrap-timepicker/css/bootstrap-timepicker.min.css">
    <link rel="stylesheet" href="~/FrontEnd/Theme/bundles/datatables/datatables.min.css">
    <link rel="stylesheet" href="~/FrontEnd/Theme/bundles/datatables/DataTables-1.10.16/css/dataTables.bootstrap4.min.css">

    <!-- Template CSS -->
    <link rel="stylesheet" href="~/FrontEnd/Theme/css/style.css">
    <link rel="stylesheet" href="~/FrontEnd/Theme/bundles/pretty-checkbox/pretty-checkbox.min.css">
    <link rel="stylesheet" href="~/FrontEnd/Theme/css/components.css">
    <!-- Custom style CSS -->
    <link rel="stylesheet" href="~/FrontEnd/Theme/css/custom.css">
    <link rel="stylesheet" href="~/FrontEnd/Theme/bundles/izitoast/css/iziToast.min.css">

    <link rel="stylesheet" href="~/FrontEnd/Theme/bundles/select2/dist/css/select2.min.css">
    <link rel="stylesheet" href="~/FrontEnd/Theme/bundles/jquery-selectric/selectric.css">

    <link rel="stylesheet" href="~/FrontEnd/Theme/bundles/bootstrap-timepicker/css/bootstrap-timepicker.min.css">
    <link rel="stylesheet" href="~/FrontEnd/Theme/bundles/bootstrap-daterangepicker/daterangepicker.css">

    <link rel="icon" type="image/png" href="~/Content/images/favicon.png">

    <link rel="stylesheet" href="~/FrontEnd/Theme/bundles/summernote/summernote-bs4.css">


    @RenderSection("styles", required: false)



</head>

<body class="sidebar-mini">
    <div class="loader"></div>
    <div id="app">
        <div class="main-wrapper main-wrapper-1">
            <div class="navbar-bg"></div>
            <nav class="navbar navbar-expand-lg main-navbar sticky">
                <div class="form-inline mr-auto">
                    <ul class="navbar-nav mr-3">
                        <li>
                            <a href="#" data-toggle="sidebar" class="nav-link nav-link-lg
									collapse-btn"><i data-feather="align-justify"></i></a>
                        </li>
                        <li>
                            <a href="#" class="nav-link nav-link-lg fullscreen-btn">
                                <i data-feather="maximize"></i>
                            </a>
                        </li>
                        <li>
                            <h5 style="margin:10px; color: #6777ef;"></h5>
                        </li>
                    </ul>
                </div>
                <ul class="navbar-nav navbar-right">

                    <li class="dropdown">
                        <a href="#" data-toggle="dropdown"
                           class="nav-link dropdown-toggle nav-link-lg nav-link-user">
                            <img alt="image" src="~/FrontEnd/Theme/img/users/user-8.png"
                                 class="user-img-radious-style"> <span class="d-sm-none d-lg-inline-block"></span>
                        </a>
                        @Html.Partial("_AccountOption")
                    </li>
                </ul>
            </nav>
            @Html.Partial("_Menu")
            <!-- Main Content -->
            <div class="main-content">


                <section class="section">
                    <div class="section-body">
                        @Html.Partial("_Notifications")
                        @RenderBody()
                    </div>
                </section>
                <div class="settingSidebar">
                    <a href="javascript:void(0)" class="settingPanelToggle">
                        <i class="fa fa-spin fa-cog"></i>
                    </a>
                    <div class="settingSidebar-body ps-container ps-theme-default">
                        <div class="fade show active">
                            <div class="setting-panel-header">
                                Setting Panel
                            </div>
                            <div class="p-15 border-bottom">
                                <h6 class="font-medium m-b-10">Select Layout</h6>
                                <div class="selectgroup layout-color w-50">
                                    <label class="selectgroup-item">
                                        <input type="radio" name="value" value="1" class="selectgroup-input-radio select-layout" checked>
                                        <span class="selectgroup-button">Light</span>
                                    </label>
                                    <label class="selectgroup-item">
                                        <input type="radio" name="value" value="2" class="selectgroup-input-radio select-layout">
                                        <span class="selectgroup-button">Dark</span>
                                    </label>
                                </div>
                            </div>
                            <div class="p-15 border-bottom">
                                <h6 class="font-medium m-b-10">Sidebar Color</h6>
                                <div class="selectgroup selectgroup-pills sidebar-color">
                                    <label class="selectgroup-item">
                                        <input type="radio" name="icon-input" value="1" class="selectgroup-input select-sidebar">
                                        <span class="selectgroup-button selectgroup-button-icon" data-toggle="tooltip"
                                              data-original-title="Light Sidebar"><i class="fas fa-sun"></i></span>
                                    </label>
                                    <label class="selectgroup-item">
                                        <input type="radio" name="icon-input" value="2" class="selectgroup-input select-sidebar" checked>
                                        <span class="selectgroup-button selectgroup-button-icon" data-toggle="tooltip"
                                              data-original-title="Dark Sidebar"><i class="fas fa-moon"></i></span>
                                    </label>
                                </div>
                            </div>
                            <div class="p-15 border-bottom">
                                <h6 class="font-medium m-b-10">Color Theme</h6>
                                <div class="theme-setting-options">
                                    <ul class="choose-theme list-unstyled mb-0">
                                        <li title="white" class="active">
                                            <div class="white"></div>
                                        </li>
                                        <li title="cyan">
                                            <div class="cyan"></div>
                                        </li>
                                        <li title="black">
                                            <div class="black"></div>
                                        </li>
                                        <li title="purple">
                                            <div class="purple"></div>
                                        </li>
                                        <li title="orange">
                                            <div class="orange"></div>
                                        </li>
                                        <li title="green">
                                            <div class="green"></div>
                                        </li>
                                        <li title="red">
                                            <div class="red"></div>
                                        </li>
                                    </ul>
                                </div>
                            </div>
                            <div class="p-15 border-bottom">
                                <div class="theme-setting-options">
                                    <label class="m-b-0">
                                        <input type="checkbox" name="custom-switch-checkbox" class="custom-switch-input"
                                               id="mini_sidebar_setting">
                                        <span class="custom-switch-indicator"></span>
                                        <span class="control-label p-l-10">Mini Sidebar</span>
                                    </label>
                                </div>
                            </div>
                            <div class="p-15 border-bottom">
                                <div class="theme-setting-options">
                                    <label class="m-b-0">
                                        <input type="checkbox" name="custom-switch-checkbox" class="custom-switch-input"
                                               id="sticky_header_setting">
                                        <span class="custom-switch-indicator"></span>
                                        <span class="control-label p-l-10">Sticky Header</span>
                                    </label>
                                </div>
                            </div>
                            <div class="mt-4 mb-4 p-3 align-center rt-sidebar-last-ele">
                                <a href="#" class="btn btn-icon icon-left btn-primary btn-restore-theme">
                                    <i class="fas fa-undo"></i> Restore Default
                                </a>
                            </div>
                        </div>
                    </div>
                </div>

                <div id="myPV"></div>
                @Html.Partial("_LoaderLayout")
            </div>
            <footer class="main-footer">
                <div class="footer-left">
                    &copy; @DateTime.Now.Year -

                    @*<a href="http://www.iciltek.com/" target="_blank">Developed by ICILtek</a>*@
                </div>
                <div class="footer-right">
                </div>
            </footer>
        </div>
    </div>

    <!-- General JS Scripts -->
    <script src="~/FrontEnd/Theme/js/app.min.js"></script>
    <script src="~/FrontEnd/Theme/bundles/izitoast/js/iziToast.min.js"></script>

    <!-- JS Libraies -->
    <script src="~/FrontEnd/Theme/bundles/datatables/datatables.min.js"></script>
    <script src="~/FrontEnd/Theme/bundles/datatables/DataTables-1.10.16/js/dataTables.bootstrap4.min.js"></script>

    <!-- Template JS File -->
    <script src="~/FrontEnd/Theme/js/scripts.js"></script>
    <!-- Custom JS File -->
    <script src="~/FrontEnd/Theme/js/custom.js"></script>

    <script src="~/FrontEnd/Theme/js/page/toastr.js"></script>
    <script src="~/FrontEnd/Theme/bundles/select2/dist/js/select2.full.min.js"></script>
    <script src="~/FrontEnd/Theme/bundles/jquery-selectric/jquery.selectric.min.js"></script>

    <script src="~/FrontEnd/Theme/bundles/jquery-validation/dist/jquery.validate.min.js"></script>

    <script src="~/FrontEnd/Theme/bundles/jquery-steps/jquery.steps.min.js"></script>


    <script src="~/FrontEnd/Theme/bundles/bootstrap-daterangepicker/daterangepicker.js"></script>
    <script src="~/FrontEnd/Theme/bundles/bootstrap-timepicker/js/bootstrap-timepicker.min.js"></script>

    <script src="~/FrontEnd/Theme/bundles/sweetalert/sweetalert.min.js"></script>

    <script src="~/FrontEnd/Theme/bundles/editable-table/mindmup-editabletable.js"></script>
    <script src="~/FrontEnd/Theme/js/page/editable-table.js"></script>

    <script src="~/FrontEnd/Theme/bundles/summernote/summernote-bs4.js"></script>

    
    <!-- Page Specific JS File -->
    @*<script src="~/FrontEnd/Theme/bundles/page/light-gallery.js"></script>*@
  
    <script src="~/FrontEnd/Theme/bundles/lightgallery/dist/js/lightgallery-all.js"></script>


    <!-- Page Specific JS File -->
    @*<script src="~/FrontEnd/Theme/js/page/forms-advanced-forms.js"></script>*@



    <script type="text/javascript">


        $(function () {
            $.fn.serializeObject = function () {

                var form = {};
                $.each($(this).serializeArray(), function (i, field) {
                    form[field.name] = field.value || "";
                });

                return form;
            };
        });
        function setParam(uri, key, val) {
            return uri
                .replace(RegExp("([?&]" + key + "(?=[=&#]|$)[^#&]*|(?=#|$))"), "&" + key + "=" + encodeURIComponent(val))
                .replace(/^([^?&]+)&/, "$1?");
        }








        function toastrInfo(message) {
            iziToast.info({
                title: 'Info',
                message: message,
                timeout: 15000,
                close: true,
                position: 'topCenter'
            });
        }


        function toastrSuccess(message) {
            iziToast.success({
                title: 'Success',
                message: message,
                timeout: 15000,
                close: true,
                position: 'topCenter'
            });
        }


        function toastrWarning(message) {
            iziToast.warning({
                title: 'Warning',
                message: message,
                timeout: 15000,
                close: true,
                position: 'topCenter'
            });
        }


        function toastrError(message) {
            iziToast.error({
                title: 'Error',
                message: message,
                timeout: 15000,
                close: true,
                position: 'topCenter'
            });
        }


        function applyDatatableClass(tableId) {
            $(tableId).dataTable();
        }

        function showHideLoaderSaad(value, divId) {
            if (value == true)
                $(divId).show();
            else
                $(divId).hide();
        }

        function toggleModalSaad(idOrClass) {

            $(idOrClass).modal('toggle');
        }

        function toggleUniversalLoader() {

            $("#UniversalLoaderModal").modal('toggle');
        }

        function showHideProgressButtonSaad(sender, condition) {

            if (condition) {
                $(sender).addClass("btn-progress");
                $(sender).attr("disabled", true);
            }
            else {
                $(sender).removeClass("btn-progress");
                $(sender).attr("disabled", false);
            }
        }


        function insertText(message, idOrClass) {

            $(idOrClass).html("<p>" + message + "</p>");
        }

 

    </script>

    @RenderSection("scripts", required: false)
</body>


</html>
