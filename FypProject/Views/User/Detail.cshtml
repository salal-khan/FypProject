@model FypProject.Models.DetailUserVM

@{
    ViewBag.Title = "Detail";
}
 <section class="section">
    <div class="section-body">
        <div class="row">
            <div class="col-12">
                <div class="card">
                    <div class="card-header">
                        <h4>User Detail</h4>
                    </div>

                    <div class="card-body">
                        <div class="box box-primary">
                            <div class="box-body box-profile">
                                <h5 class="text-muted text-center"><strong> @Model.FirstName @Model.LastName</strong></h5>

                                <ul class="list-group list-group-unbordered">

                                    <li class="list-group-item">
                                        <b>User Name</b>
                                        <strong> <a class="pull-right">@Model.UserName</a></strong>
                                    </li>

                                    <li class="list-group-item">
                                        <b>First Name</b>
                                        <strong> <a class="pull-right">@Model.FirstName</a></strong>
                                    </li>


                                    <li class="list-group-item">
                                        <b>Last Name</b>
                                        <strong> <a class="pull-right">@Model.LastName</a></strong>
                                    </li>


                                    <li class="list-group-item">
                                        <b>Email</b>
                                        <strong> <a class="pull-right">@Model.EmailAddress</a></strong>
                                    </li>

                                    <li class="list-group-item">
                                        <b>CNIC</b>
                                        <strong> <a class="pull-right">@Model.Cnic</a></strong>
                                    </li>


                                    <li class="list-group-item">
                                        <b>Phone Number</b>
                                        <strong> <a class="pull-right">@Model.PhoneNumber</a></strong>
                                    </li>



                                    <li class="list-group-item">
                                        <b>Role</b>
                                        <strong> <a class="pull-right">@Model.RoleName</a></strong>
                                    </li>

                                    <li class="list-group-item">
                                        <b>Address</b>
                                        <strong> <a class="pull-right">@Model.Address</a></strong>
                                    </li>
                                 
                                </ul>

                            </div>

                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

